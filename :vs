package main

import "fmt"

type deck []string

func newDeck() deck {
    return deck{"Ace of Spades"}
}

// This is a function RECEIVER 
// it means that any variable of type "deck"
// now has the access to the "print" method
func (d deck) print() {
    for i, card := range d {
        fmt.Println(i, card)
    }
}
